{
  "extends": "./tsconfig.options.json",
  "compilerOptions": {
		"emitDeclarationOnly": false,
		"noEmit": true
  },
  "parserOptions": {
    "extraFileExtensions": [".mjs"]
  },
  "include": [
    "**/.eslintrc.cjs",
    "website/src/**/*",
    "website/*.js",
    "website/*.ts",
  ],
  "exclude": [
    "**/*.css"
  ]
}
