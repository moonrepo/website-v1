{
  "extends": [
    "../tsconfig.options.json",
    "@docusaurus/tsconfig"
  ],
  "compilerOptions": {
    "emitDeclarationOnly": true,
    "noEmit": false,
    "outDir": "./build",
    "verbatimModuleSyntax": false,
    "baseUrl": ".",
    "paths": {
      "@site/*": [
        "./*"
      ]
    }
  },
  "include": [
    ".eslintrc.cjs",
    "*.config.js",
    "*.config.ts",
    "sidebars.ts",
    "src/**/*"
  ],
  "exclude": [
    "**/*.mdx"
  ]
}
